<div class="col-md-4 col-md-offset-4">
  <h2>編輯討論版</h2>
  <hr>

   <%= form_for @group do |f| %>
   <%= f.label :title, "標題" %>
   <%= f.text_field :title %>
   <br>
   <%= f.label :description, "敘述" %>
   <%= f.text_field :description %>
   <br>
   <%= f.submit "Submit", :disable_with => 'Submitting...'%>
   <% end %>
</div>